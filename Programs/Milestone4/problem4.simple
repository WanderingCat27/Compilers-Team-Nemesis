define void sqrt_newton (double x, int iterations) {
    distance = 0.0
    y = 0.0
    repeat iterations {
        print "input an end point"
        input decimal y
        distance = (x multiply x) plus (y multiply y)
        distance = square root distance
        print "distance:"
        print distance
    }
}
